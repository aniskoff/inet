<scenario>
    <at t="0.2">
        <set-param module="switch1.gptp" par="grandMaster" value="false"/>
        <set-param module="switch1.gptp" par="masterPorts" value='["eth1","eth2"]'/>
    </at>
    <at t="0.25">
        <set-param module="switch2.gptp" par="slavePort" value='""'/>
        <set-param module="switch2.gptp" par="grandMaster" value="true"/>
        <set-param module="switch2.gptp" par="masterPorts" value='["eth0"]'/>

        <set-param module="switch1.gptp" par="slavePort" value='"eth0"'/>
    </at>
</scenario>
